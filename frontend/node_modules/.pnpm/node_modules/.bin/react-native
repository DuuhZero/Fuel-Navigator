#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/eduardo/Desktop/FATEC/MobileApp/FuelNavigation/frontend/node_modules/.pnpm/@react-native-community+cli@11.3.7_@babel+core@7.28.5/node_modules/@react-native-community/cli/build/node_modules:/home/eduardo/Desktop/FATEC/MobileApp/FuelNavigation/frontend/node_modules/.pnpm/@react-native-community+cli@11.3.7_@babel+core@7.28.5/node_modules/@react-native-community/cli/node_modules:/home/eduardo/Desktop/FATEC/MobileApp/FuelNavigation/frontend/node_modules/.pnpm/@react-native-community+cli@11.3.7_@babel+core@7.28.5/node_modules/@react-native-community/node_modules:/home/eduardo/Desktop/FATEC/MobileApp/FuelNavigation/frontend/node_modules/.pnpm/@react-native-community+cli@11.3.7_@babel+core@7.28.5/node_modules:/home/eduardo/Desktop/FATEC/MobileApp/FuelNavigation/frontend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/eduardo/Desktop/FATEC/MobileApp/FuelNavigation/frontend/node_modules/.pnpm/@react-native-community+cli@11.3.7_@babel+core@7.28.5/node_modules/@react-native-community/cli/build/node_modules:/home/eduardo/Desktop/FATEC/MobileApp/FuelNavigation/frontend/node_modules/.pnpm/@react-native-community+cli@11.3.7_@babel+core@7.28.5/node_modules/@react-native-community/cli/node_modules:/home/eduardo/Desktop/FATEC/MobileApp/FuelNavigation/frontend/node_modules/.pnpm/@react-native-community+cli@11.3.7_@babel+core@7.28.5/node_modules/@react-native-community/node_modules:/home/eduardo/Desktop/FATEC/MobileApp/FuelNavigation/frontend/node_modules/.pnpm/@react-native-community+cli@11.3.7_@babel+core@7.28.5/node_modules:/home/eduardo/Desktop/FATEC/MobileApp/FuelNavigation/frontend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@react-native-community/cli/build/bin.js" "$@"
else
  exec node  "$basedir/../@react-native-community/cli/build/bin.js" "$@"
fi
